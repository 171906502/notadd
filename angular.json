{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "noxus-sso": {
      "root": "apps/noxus/sso",
      "sourceRoot": "apps/noxus/sso/src",
      "projectType": "application",
      "prefix": "noxus-sso",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/noxus/sso",
            "main": "apps/noxus/sso/src/main.ts",
            "tsConfig": "apps/noxus/sso/tsconfig.app.json",
            "assets": ["apps/noxus/sso/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/noxus/sso/src/environments/environment.ts",
                  "with": "apps/noxus/sso/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "noxus-sso:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/noxus/sso/tsconfig.app.json",
              "apps/noxus/sso/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/noxus/sso/jest.config.js",
            "tsConfig": "apps/noxus/sso/tsconfig.spec.json"
          }
        }
      }
    },
